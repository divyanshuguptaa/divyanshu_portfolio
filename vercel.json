{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "flight_delay_dashboard.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/dashboard",
      "dest": "/flight_delay_dashboard.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

